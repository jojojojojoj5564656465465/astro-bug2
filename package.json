{
  "name": "marina",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "wireit",
    "build": "wireit",
    "preview": "wireit",
    "wrangler": "wireit",
    "lint:scss": "wireit",
    "check-eslint": "wireit",
    "format": "wireit"
  },
  "wireit": {
    "start": {
      "command": "astro dev",
      "files": [
        "unocss.config.ts"
      ],
      "dependencies": [
        "format"
      ]
    },
    "build": {
      "command": "astro build",
      "files": [
        "unocss.config.ts"
      ]
    },
    "wrangler": {
      "command": "nix wrangler pages deploy ./dist",
      "dependencies": [
        "format",
        "build"
      ]
    },
    "preview": {
      "command": "astro preview",
      "dependencies": [
        "format",
        "build"
      ]
    },
    "check-eslint": {
      "command": "eslint --color --cache --cache-location .eslintcache --fix .",
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.json"
      ],
      "output": []
    },
    "lint:scss": {
      "command": "stylelint --fix \"src/**/*.{astro,scss,css}\""
    },
    "format": {
      "command": "prettier --ignore-path .prettierignore --config .prettierrc.cjs --cache --write ."
    }
  },
  "dependencies": {
    "@astrojs/cloudflare": "^6.5.1",
    "@astrojs/image": "^0.16.9",
    "@fontsource-variable/playfair-display": "^5.0.4",
    "@fontsource-variable/yrsa": "^5.0.4",
    "@fontsource/hind-guntur": "^5.0.3",
    "@fontsource/rochester": "^5.0.3",
    "astro": "^2.7.2",
    "astro-icon": "^0.8.1",
    "astro-seo": "^0.7.4",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/sitemap": "^1.3.3",
    "@iconify/json": "^2.2.84",
    "@total-typescript/ts-reset": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@unocss/eslint-config": "^0.53.4",
    "@unocss/preset-attributify": "^0.53.4",
    "@unocss/preset-icons": "^0.53.4",
    "@unocss/preset-typography": "^0.53.4",
    "@unocss/preset-web-fonts": "^0.53.4",
    "@unocss/preset-wind": "^0.53.4",
    "@unocss/reset": "^0.53.4",
    "@unocss/transformer-directives": "^0.53.4",
    "@unocss/transformer-variant-group": "^0.53.4",
    "astro-compress": "^1.1.47",
    "astro-critters": "^1.1.38",
    "astro-imagetools": "^0.8.1",
    "astro-robots-txt": "^0.5.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^8.5.1",
    "postcss-scss": "^4.0.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-astro": "^0.10.0",
    "rome": "^12.1.3",
    "stylelint": "^15.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^12.0.0",
    "unocss": "^0.53.4",
    "wireit": "^0.9.5",
    "wrangler": "^3.1.1"
  }
}
